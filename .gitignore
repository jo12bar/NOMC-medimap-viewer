# Local config, startup script, etc...
local

# Generated build files.
static/css/styles.css
static/css/styles.css.map
static/js/bundle.js
static/js/bundle.js.map

# All fonts should be required by webpack. They'll probably be copied here by
# the build process, so we can safely ignore them.
static/fonts

# Ignore all imported photos, except for the test ones.
static/raspi-imported-photos/*
!static/raspi-imported-photos/README.md
!static/raspi-imported-photos/test-*.jpg
!static/raspi-imported-photos/test-*.jpeg
!static/raspi-imported-photos/test-*.JPG
!static/raspi-imported-photos/test-*.JPEG
!static/raspi-imported-photos/test-*.png
!static/raspi-imported-photos/test-*.gif

# Ignore the file that the production server uses to tell the startup script
# that it's running. The startup script is supposed to remove this, but devs
# testing the production server might forget to remove it manually. This keeps
# them from accidentally committing it :)
.node-server-is-running

# Logs
logs
*.log
npm-debug.log*

# Runtime data
pids
*.pid
*.seed

# Directory for instrumented libs generated by jscoverage/JSCover
lib-cov

# Coverage directory used by tools like istanbul
coverage

# nyc test coverage
.nyc_output

# Grunt intermediate storage (http://gruntjs.com/creating-plugins#storing-task-files)
.grunt

# node-waf configuration
.lock-wscript

# Compiled binary addons (http://nodejs.org/api/addons.html)
build/Release

# Dependency directories
node_modules
jspm_packages

# Optional npm cache directory
.npm

# Optional REPL history
.node_repl_history
